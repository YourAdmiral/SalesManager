<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="FSharp.Core" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-4.3.0.0" newVersion="4.3.0.0" />
			</dependentAssembly>
		</assemblyBinding>
	</runtime>
	<connectionStrings>
		<add name="SalesDbConnectionString" connectionString="Data source=(localdb)\MSSQLLocalDB; Initial Catalog=SalesDB; Integrated Security=True;" providerName="System.Data.SqlClient" />
	</connectionStrings>
	<appSettings>
		<add key="SalesSourcePath" value="e:/Temp/Sales" />
		<add key="SalesReadyFilesPath" value="e:/Temp/Sales/ReadyFiles" />
		<add key="SalesErrorFilesPath" value="e:/Temp/Sales/ErrorFiles" />
	</appSettings>
</configuration>